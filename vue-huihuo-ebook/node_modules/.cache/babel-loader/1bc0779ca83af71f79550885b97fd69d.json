KAAA,IAAA,CAAA,KAAA,KAAA,GAAA,KAAA,KAAA,CAFA,EAGA,KAAA,IAAA,CAAA,KAAA,KAAA,GAAA,KAAA,KAAA,GAAA,CAAA,CAHA,CAAA;AAKA,OANA,MAMA;AACA,eAAA,EAAA;AACA;AACA;AAdA,GAbA;AA6BA,EAAA,IA7BA,kBA6BA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,CADA;AAEA,MAAA,KAAA,EAAA;AAFA,KAAA;AAIA,GAlCA;AAmCA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,UAAA,KAAA,KAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,aAAA,KAAA,GAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,KAAA;AACA;AACA,KAPA;AAQA,IAAA,UARA,sBAQA,IARA,EAQA;AACA,UAAA,IAAA,IAAA,IAAA,CAAA,IAAA,IAAA,IAAA,CAAA,MAAA,EAAA;AACA,gBAAA,IAAA,CAAA,IAAA;AACA,eAAA,CAAA;AACA,mBAAA,KAAA,EAAA,CAAA,iBAAA,EAAA,OAAA,CAAA,IAAA,EAAA,IAAA,CAAA,MAAA,CAAA;;AACA,eAAA,CAAA;AACA,mBAAA,KAAA,EAAA,CAAA,iBAAA,EAAA,OAAA,CAAA,IAAA,EAAA,IAAA,CAAA,MAAA,CAAA;;AACA,eAAA,CAAA;AACA,mBAAA,KAAA,EAAA,CAAA,kBAAA,EACA,OADA,CACA,IADA,EACA,IAAA,CAAA,OADA,EAEA,OAFA,CAEA,IAFA,EAEA,IAAA,CAAA,MAFA,CAAA;AANA;AAUA;AACA,KArBA;AAsBA,IAAA,MAtBA,oBAsBA;AAAA;;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,KAAA;AACA,SAFA;;AAGA,QAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,KAAA;AACA,SAFA;;AAGA,QAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,KAAA;AACA,SAFA;AAGA,OAVA;AAWA;AAlCA;AAnCA,CAAA","sourcesContent":["<template>\n  <div class=\"guess-you-like\">\n    <title-view\n      :label=\"$t('home.guessYouLike')\"\n      :btn=\"$t('home.change')\"\n      @onClick=\"change\"\n    ></title-view>\n    <div class=\"guess-you-like-list\">\n      <div\n        class=\"guess-you-like-item\"\n        v-for=\"(item, index) in showData\"\n        :key=\"index\"\n        @click=\"showBookDetail(item)\"\n      >\n        <div class=\"img-wrapper\">\n          <img class=\"img\" :src=\"item.cover\" />\n        </div>\n        <div class=\"content-wrapper\">\n          <div class=\"title title-big\" ref=\"title\">{{item.title}}</div>\n          <div class=\"author sub-title\" ref=\"author\">{{item.author}}</div>\n          <div class=\"result third-title\" ref=\"result\">{{resultText(item)}}</div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script type=\"text/ecmascript-6\">\n  import TitleView from './Title'\n  import { realPx } from '../../utils/utils'\n  import { homeMixin } from '../../utils/mixin'\n\n  export default {\n    mixins: [homeMixin],\n    components: {\n      TitleView\n    },\n    props: {\n      data: Array\n    },\n    watch: {\n      data(v) {\n        this.total = v.length / 3\n      }\n    },\n    computed: {\n      width() {\n        return window.innerWidth - realPx(20) - realPx(60) + 'px'\n      },\n      showData() {\n        if (this.data) {\n          return [\n            this.data[this.index],\n            this.data[this.index + this.total],\n            this.data[this.index + this.total * 2]\n          ]\n        } else {\n          return []\n        }\n      }\n    },\n    data() {\n      return {\n        index: 0,\n        total: 0\n      }\n    },\n    methods: {\n      change() {\n        if (this.index + 1 >= this.total) {\n          this.index = 0\n        } else {\n          this.index++\n        }\n      },\n      resultText(item) {\n        if (item && item.type && item.result) {\n          switch (item.type) {\n            case 1:\n              return this.$t('home.sameAuthor').replace('$1', item.result)\n            case 2:\n              return this.$t('home.sameReader').replace('$1', item.result)\n            case 3:\n              return this.$t('home.readPercent')\n                .replace('$1', item.percent)\n                .replace('$2', item.result)\n          }\n        }\n      },\n      resize() {\n        this.$nextTick(() => {\n          this.$refs.title.forEach(item => {\n            item.style.width = this.width\n          })\n          this.$refs.author.forEach(item => {\n            item.style.width = this.width\n          })\n          this.$refs.result.forEach(item => {\n            item.style.width = this.width\n          })\n        })\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" rel=\"stylesheet/scss\" scoped>\n  @import '../../assets/styles/global';\n\n  .guess-you-like {\n    .guess-you-like-list {\n      width: 100%;\n      padding: 0 px2rem(10);\n      box-sizing: border-box;\n      .guess-you-like-item {\n        display: flex;\n        margin-top: px2rem(15);\n        &:first-child {\n          margin-top: px2rem(5);\n        }\n        .img-wrapper {\n          flex: 0 0 20%;\n          padding: px2rem(10) px2rem(10) px2rem(10) 0;\n          box-sizing: border-box;\n          .img {\n            width: 100%;\n          }\n        }\n        .content-wrapper {\n          flex: 1;\n          padding: px2rem(10) 0;\n          box-sizing: border-box;\n          text-align: left;\n          .author {\n            margin-top: px2rem(15);\n          }\n          .result {\n            margin-top: px2rem(5);\n          }\n        }\n      }\n    }\n  }\n</style>\n\n"],"sourceRoot":"src/components/home"}]}